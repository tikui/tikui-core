{
  "name": "@tikui/core",
  "version": "6.3.0",
  "description": "Tikui core",
  "main": "src/tikui.js",
  "scripts": {
    "build": "npm run clean & tsc",
    "clean": "rimraf dist",
    "component-test:start": "TIKUI_PATH='testing-tikui' node dist/tikui-core.js serve",
